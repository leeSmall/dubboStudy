<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" 
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xmlns:jack="http://www.dongnaoedu.com/schema/soa"
       xsi:schemaLocation="http://www.springframework.org/schema/beans 
       http://www.springframework.org/schema/beans/spring-beans-3.2.xsd
	   http://www.dongnaoedu.com/schema/soa 
	   http://www.dongnaoedu.com/schema/soa.xsd"
       default-lazy-init="true">
	<bean id="providerService" class="com.study.service.ProviderServiceImpl"></bean>
	<bean id="userService" class="com.study.service.UserServiceImpl"></bean>
 	<jack:protocol id="protocol" name="netty" port="1123" host="127.0.0.1" contextpath="/dubbo-provider-web/api/httpService"></jack:protocol>
 	<jack:service id="service1" interface="com.study.service.ProviderService" ref="providerService"></jack:service>
 	<jack:service id="service2" interface="com.study.service.UserService" ref="userService"></jack:service>
 	<jack:registry id="registry" protocol="redis" address="127.0.0.1:6379"></jack:registry>
 	<!--<jack:reference id="providerService" interface="com.study.service.ProviderService" loadbalance="random" protocol="http"></jack:reference>
 	<jack:reference id="userService" interface="com.study.service.UserService" loadbalance="random" protocol="http"></jack:reference>-->
</beans>  